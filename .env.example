# Appwrite environment variables

# Your Appwrite API endpoint URL (e.g. https://cloud.appwrite.io/v1)
VITE_APPWRITE_ENDPOINT="your APPWRITE ENDPOINT"

# Your Appwrite project ID (found in your Appwrite console)
VITE_APPWRITE_PROJECT_ID="your APPWRITE PROJECT ID"

# Your Appwrite database ID (from your Appwrite database service)
VITE_APPWRITE_DATABASE_ID="your APPWRITE DATABASE ID"

# Collection ID for skills data (from your Appwrite database collections)
VITE_APPWRITE_SKILLS_COLLECTION_ID="your SKILLS COLLECTION ID"

# Collection ID for certificates data
VITE_APPWRITE_CERTIFICATES_COLLECTION_ID="your CERTIFICATES COLLECTION ID"

# Collection ID for projects data
VITE_APPWRITE_PROJECTS_COLLECTION_ID="your PROJECTS COLLECTION ID"

# Collection ID for testimonials data
VITE_APPWRITE_TESTIMONIALS_COLLECTION_ID="your TESTIMONIALS COLLECTION ID"

# Storage bucket ID (for file uploads, e.g. profile pictures)
VITE_APPWRITE_BUCKET_ID="your BUCKET ID"

# Web3Forms environment variables

# Your Web3Forms access key (for form submission API)
WEB3FORMS_ACCESS_KEY="your WEB3FORMS ACCESS KEY"
